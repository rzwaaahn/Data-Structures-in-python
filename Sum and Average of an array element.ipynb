   "source": [
    "# Find the SUM and AVERAGE of an array element \n",
    "\n",
    "\n",
    "import numpy as np\n",
    "x=np.array([10,20,30,40,50])\n",
    "def avg(x):\n",
    "    l=len(x)\n",
    "    add_all=sum(x)\n",
    "    average=add_all/l\n",
    "    return average\n",
    "\n",
    "result=avg(x)\n",
    "print(result)"
   ]
